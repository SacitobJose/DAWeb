extends layout

block content
	.w3-container.w3-section
		.w3-card4
			header.w3-container.w3-teal
				h1 Lista de Entidades
		
			.w3-section
				table.w3-table-all
					tr
						th Sigla
						th Designação
						th Operações
					each e in entidades
						tr
							td= e.sigla
							td= e.designacao
							td
								button.w3-btn.w3-teal(onclick="window.location.assign('/entidades/" + e.id + "')") Consultar
